<script>
	var originalWidth = 600;
	var originalHeight = 400;
	function setOriginalImageSize(width, height) {
		originalWidth = width;
		originalHeight = height;
	}
	setOriginalImageSize(600, 400);
	
	var jpg_quality = 90;
	function setQualityVisibility(){
		if($('#selector_extension').val()=='jpeg'){
			$('#sector_calidad input').prop('disabled', false);
			$('#sector_calidad input').val(jpg_quality);
		}else{
			$('#sector_calidad input').prop('disabled', true);
			$('#sector_calidad input').val("100");
		}
	}
	
	var aspectLocked = true;
	function switchLock() {
		aspectLocked = !aspectLocked;
		if(aspectLocked) {
			$('.lock-aspect').addClass('closed');
			$('.lock-aspect').removeClass('opened');
			updateWidthHeight(document.getElementById("width_field"));
		}else{
			$('.lock-aspect').addClass('opened');
			$('.lock-aspect').removeClass('closed');
		}
	}
	
	var selectedMode="stretch";
	function selectMode(mode) {
		selectedMode = mode;
		$('.resize-mode').removeClass("selected");
		$('.resize-mode-'+selectedMode).addClass("selected");
	}
	
	function updateWidthHeight(field) {
		if(aspectLocked) {
			if($('#selector_units').val()=="percent"){
				other_field_name = field.id == "width_field" ? 'height' : 'width';
    			$('#'+other_field_name+'_field').val(field.value);
			} else {
				if (field.id == "width_field") {
					$('#height_field').val(Math.round(originalHeight * field.value / originalWidth));
				} else {
					$('#width_field').val(Math.round(originalWidth * field.value / originalHeight));
				}
			}
		}
		showHideModes();
	}
	
	function convertBetweenPixelsAndPercentage(){
		if($('#selector_units').val()=="percent"){
			$('#width_field').val(70);
			$('#height_field').val(70);
		} else if($('#selector_units').val()=="pixels"){
			$('#width_field').val(originalWidth);
			$('#height_field').val(originalHeight);
		}
		showHideModes();
	}
	
	function showHideModes() {
		if(!aspectLocked && $("#width_field").val() != $("#height_field").val()) {
			$(".resize-mode-area").slideDown();
			$(".resize-mode-separator").slideDown();
		} else {
			$(".resize-mode-area").slideUp();
			$(".resize-mode-separator").slideUp();
		}
	}
</script>    					
            				
